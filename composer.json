{
    "name": "railken/lara-ore-custom-page",
    "keywords": ["laravel", "lara-ore", "lara-ore-custom-page", "custom-page"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/bag": "^1.2",
        "railken/laravel-manager": "^3.0.3",
        "fzaninotto/faker": "^1.7",
        "railken/lara-ore-api": "dev-master",
        "railken/lara-ore-support": "dev-master",
        "railken/lara-ore-template": "dev-master",
        "railken/lara-ore-file": "dev-master",
        "railken/lara-ore-repository": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\LaraOre\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\LaraOre\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\LaraOre\\CustomPageServiceProvider"
            ]
        }
    }
}
